

{% extends 'base.html.twig' %}
{% block titre %} Gestion des annonces {% endblock %}

{% block body %}
<div class="pcoded-content">
    <div class="pcoded-inner-content">
        <div class="main-body">
            <div class="page-wrapper">

                <div class="page-body">

                    <h3>gestion des commandes</h3>

                    <div class="page-header card">
                        <div class="card-block">
                            <h5 class="m-b-10">Ajouter commande</h5>

                            <div class="container my-3">
                                <div class="row">
                                    <div class="col-sm-12 col-lg-6">
                                        {{ form_start(form) }}
                                        <div class="form-group">
                                            {{ form_label(form.prixprod, 'prixprod') }}
                                            {{ form_widget(form.prixprod, {'attr': { 'class' : 'form-control'}}) }}
                                        </div>
                                        <div class="form-group">
                                            {{ form_label(form.prixlivr, 'prixlivr') }}
                                            {{ form_widget(form.prixlivr, {'attr': { 'class' : 'form-control'}}) }}
                                        </div>
                                        <div class="form-group">
                                        {{ form_label(form.produit, 'produit') }}
                                        {{ form_widget(form.produit, {'attr': { 'class' : 'form-control'}}) }}
                                    </div>
                                        <div class="form-group">
                                        {{ form_label(form.etat, 'etat') }}
                                        {{ form_widget(form.etat, {'attr': { 'class' : 'form-control'}}) }}
                                    </div>
                                        <div class="form-group">
                                            {{ form_widget(form.Submit, {'attr': { 'class' : 'form-control'}}) }}

                                        {{ form_end(form) }}
                                    </div>
                                </div>
                            </div></div></div></div></div>
{% endblock %}